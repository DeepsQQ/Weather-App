{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "plugins": ["prettier"],
  "extends": ["react-app", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": [0, "windows"],
    "react-hooks/exhaustive-deps": "off",
    "quotes": ["error", "double"]
  }
}
